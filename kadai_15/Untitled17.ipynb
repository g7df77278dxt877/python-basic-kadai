        "class human:\n",
        "    def __init__(self,name,age):\n",
        "        self.name=name\n",
        "        self.age=age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前は{self.name}です\")\n",
        "        print(f\"年齢は{self.age}です\")\n",
        "\n",
        "person=human(\"吉武\",\"40\")\n",
        "\n",
        "person.printinfo()\n",
        "\n",
        "\n"
