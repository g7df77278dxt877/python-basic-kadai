        "import random\n",
        "\n",
        "var=random.randint(1,15)\n",
        "\n",
        "print(var)\n",
        "\n",
        "if var%3==0:\n",
        "    print(\"Fizz\")\n",
        "elif var%5==0:\n",
        "    print(\"Buzz\")\n",
        "if var%3==0 and var%5==0:\n",
        "    print(\"FizzBuzz\")\n",
        "else:\n",
        "    print(\"var\")\n",
        "\n",
        "\n"
      ]
